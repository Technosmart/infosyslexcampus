#PF-Assgn-22
list_of_leap_years=[0]*15
def find_leap_years(given_year):
    
    # Write your logic here
    if (given_year%4==0) and (given_year%100!=0 or given_year%400==0):
        list_of_leap_years[0]=given_year
        for x in range (1,15):
            list_of_leap_years[x]= list_of_leap_years[x-1]+4
    else:
        find_leap_years(given_year+1)

    return list_of_leap_years
print(find_leap_years(1783))
